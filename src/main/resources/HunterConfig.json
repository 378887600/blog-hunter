    "header": [
      "Host=www.imooc.com",
      "Referer=https://www.imooc.com"
    ],
    "entryUrls": "https://www.imooc.com/u/{uid}/articles?page={curPage}"
  },
  "csdn": {
    "domain": "blog.csdn.net",
